{
  "name": "alecrabbit/php-console-colour",
  "type": "library",
  "description": "PHP package alecrabbit/php-console-colour",
  "license": "MIT",
  "require": {
    "php": ">=7.2",
    "alecrabbit/php-traits": "^0.0",
    "jakub-onderka/php-console-color": "^0.2.0"
  },
  "require-dev": {
    "nunomaduro/collision": "^2.1",
    "phpunit/phpunit": "^7.5",
    "symfony/var-dumper": "^4.2"
  },
  "autoload": {
    "psr-4": {
      "AlecRabbit\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AlecRabbit\\Tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "/var/packages/helpers"
    },
    {
      "type": "vcs",
      "url": "/var/packages/php-traits"
    }
  ],
  "minimum-stability": "beta",
  "prefer-stable": true
}
